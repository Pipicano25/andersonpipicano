                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <img src="~/res/dist/img/user2-160x160.jpg" class="user-image" alt="User Image">
                                <span class="hidden-xs">
                                    @Context.Session.GetString("Usuario")
                                    <small></small>
                                </span>
                            </a>
                            <ul class="dropdown-menu">
